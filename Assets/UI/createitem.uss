:root {

}

#progress-bar-container {
    width: 200px;
    height: 20px;
    background-color: #333333;
    border-radius: 10px;
    overflow: hidden; /* This is important for masking the fill */
}

#progress-bar-fill {
    height: 100%; /* Fill the parent container's height */
    background-color: #0088ff; /* Your progress color */
}